	"devDependencies": {
		"@astranova/eslint-config": "workspace:*",
		"@astranova/typescript-config": "workspace:*",
		"@types/node": "^22.13.9",
		"eslint": "^9.21.0",
		"eslint-config-prettier": "^10.0.2",
		"eslint-plugin-prettier": "^5.2.3",
		"globals": "^16.0.0",
		"prisma": "^6.4.1",
		"rimraf": "^6.0.1",
		"tsup": "^8.4.0",
		"tsx": "4.19.3",
		"typescript": "5.8.2",
		"typescript-eslint": "^8.26.0"
