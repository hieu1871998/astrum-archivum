{
	"name": "@astranova/prisma",
	"version": "1.0.0",
	"exports": {
		".": {
			"import": "./src/client.ts",
			"types": "./src/client.ts"
		}
	},
	"scripts": {
		"check-types": "tsc --noEmit",
		"db:migrate:deploy": "prisma migrate deploy",
